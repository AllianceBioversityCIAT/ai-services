# Environment variables for AICCRA Report Generator
# Copy this file to .env and fill in your actual values

# AWS Bedrock Configuration
AWS_ACCESS_KEY_ID_BR=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY_BR=your_aws_secret_key_here
AWS_REGION=us-east-1

# Knowledge Base Configuration  
KNOWLEDGE_BASE_ID=your_knowledge_base_id_here

# Other AWS Services (if needed)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
BUCKET_NAME=your_bucket_name_here

# Database Configuration (if needed)
MYSQL_DATABASE_URL=your_mysql_url_here
SUPABASE_URL=your_supabase_url_here
COLLECTION_NAME=your_collection_name_here

# Server Configuration (if needed)
SERVER=your_server_here
DATABASE=your_database_here
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here