# Environment variables for AICCRA Chatbot
# Copy this file to .env and fill in your actual values

# AWS Bedrock Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID_BR=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY_BR=your_aws_secret_key_here

# S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
BUCKET_NAME=your_bucket_name_here

# OpenSearch Configuration
AWS_ACCESS_KEY_ID_OS=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY_OS=your_aws_secret_key_here
OPENSEARCH_HOST=your_opensearch_host_here
OPENSEARCH_INDEX_NAME_CHATBOT=your_opensearch_index_name_here

# Server Configuration (if needed)
SERVER=your_server_here
DATABASE=your_database_here
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here

# Knowledge Base and Agents Configuration
KNOWLEDGE_BASE_ID=your_knowledge_base_id_here
AGENT_ID=your_agent_id_here
AGENT_ALIAS_ID=your_agent_alias_id_here
MEMORY_ID=your_memory_id_here